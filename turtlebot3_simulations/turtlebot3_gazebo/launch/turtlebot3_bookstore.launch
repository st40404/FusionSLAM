<launch>
  <arg name="model" default="$(env TURTLEBOT3_MODEL)" doc="model type [burger, waffle, waffle_pi]"/>
  <arg name="x_pos" default="-3.0"/>
  <arg name="y_pos" default="5"/>
  <arg name="z_pos" default="0.0"/>

  <!-- <arg name="x_pos" default="-3.9"/>
  <arg name="y_pos" default="-5.0"/>
  <arg name="z_pos" default="0.0"/> -->

  <!-- <arg name="x_pos" default="0.0"/>
  <arg name="y_pos" default="-0.53"/>
  <arg name="z_pos" default="0.0"/> -->

  <!-- <arg name="x_pos" default="-7.1"/>
  <arg name="y_pos" default="3.3"/>
  <arg name="z_pos" default="0.0"/> -->
  
  <!-- <arg name="x_pos" default="-7.048945"/>
  <arg name="y_pos" default="-6.261363"/>
  <arg name="z_pos" default="0.0"/> -->

  <include file="$(find gazebo_ros)/launch/empty_world.launch">
    <arg name="world_name" value="$(find aws_robomaker_bookstore_world)/worlds/bookstore.world"/>
    <!-- <arg name="world_name" value="$(find aws_robomaker_bookstore_world)/worlds/bookstore_o.world"/> -->
    <arg name="paused" value="false"/>
    <arg name="use_sim_time" value="true"/>
    <arg name="gui" default="true"/>
    <arg name="headless" value="false"/>
    <arg name="debug" value="false"/>
    <arg name="verbose" value="false"/>
  </include>


  <param name="robot_description" command="$(find xacro)/xacro --inorder $(find turtlebot3_description)/urdf/turtlebot3_$(arg model).urdf.xacro" />

  <node name="spawn_urdf" pkg="gazebo_ros" type="spawn_model" args="-urdf -model turtlebot3 -x $(arg x_pos) -y $(arg y_pos) -z $(arg z_pos) -param robot_description" />
</launch>
